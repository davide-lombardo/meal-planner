{
  "name": "meal-planner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "pnpm run build:shared && pnpm run build:all",
    "build:all": "pnpm --filter \"./apps/**\" run build",
    "build:shared": "pnpm --filter \"./libs/**\" run build",
    "build:client": "pnpm --filter \"meal-planner-client\" run build",
    "build:server": "pnpm --filter \"meal-planner-server\" run build",
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:client": "pnpm --filter \"meal-planner-client\" run dev",
    "dev:server": "pnpm --filter \"meal-planner-server\" run dev",
    "dev:watch:server": "pnpm --filter \"meal-planner-server\" run dev:watch",
    "start:server": "pnpm --filter \"meal-planner-server\" run start",
    "test:client": "pnpm --filter \"meal-planner-client\" run test",
    "test:server": "pnpm --filter \"meal-planner-server\" run test",
    "lint:all": "pnpm -r run lint",
    "test:all": "pnpm -r run test"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@kinde-oss/kinde-auth-react": "^5.6.0",
    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.1.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "express-rate-limit": "^6.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.523.0",
    "nodemailer": "^7.0.3",
    "react": "^19.1.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "tslib": "^2.3.0",
    "upath": "^2.0.1",
    "winston": "^3.17.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.1",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "^26.1.0",
    "prettier": "^2.6.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
